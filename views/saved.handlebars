<h1>Saved Headlines</h1>



<div class="container-fluid">



<nav class="navbar navbar-default" id="Name">
            <div class="container-fluid">
              
              <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
                <div id="Name">
              
                </div>
              </div>

            
              <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                  <li class="active"><a href="/">Headlines </a></li>
                  <li><a href="/saved">Saved <span class="sr-only">(current)</span></a><li><form action='/scrape?_method=GET' method="POST">
                     
                     <button type='submit' class="btn btn-default">Scrape</button>
                     </form></li></li>
                </ul>
            </div>

          </div>
</nav>
  




  <div class="panel panel-default">
    
      <div class="panel-body">
          
               
              <ol>
                
                  {{#each headline}}
                    {{#if saved}}
                      <li>
                      {{title}}
                      {{link}}
                      <form class="Save-Delete-Form">
                     <input type="hidden" class="Savedheadline_id" type="text" value={{this.id}}><br>
                     <button type='submit' class="btn btn-default">Delete From Saved Articles</button>
                     </form>
                                      
                    <div id="myModal" class="modal">

                           <!-- Modal content -->
                          <div class="modal-content">
                           {{!-- <span class="close">&times;</span> --}}
                           <div id="notes"></div>
                          </div>

                    </div>
                  
                  <form class="Note-Form">
                  <button type="submit" id="Note-Button" class="btn btn-default">Add Note</button>
                  </form>
                  </li>
                   {{/if}}
                 {{/each}}

             </ol>
                
              
      </div>
  </div>

  
</div>